{
  "name": "worldcup",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "schema:download": "apollo client:download-schema --endpoint=https://eplusworldcup.loca.lt/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "axios": "^1.1.3",
    "cross-env": "^7.0.3",
    "graphql": "^15.5.1",
    "js-cookie": "^3.0.1",
    "next": "12.3.0",
    "next-auth": "^4.17.0",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-modal": "^3.16.1",
    "styled-components": "^5.3.5",
    "swiper": "^8.4.5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "6.5.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.27",
    "@types/react-modal": "^3.13.1",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "apollo": "^2.33.4",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^7.0.2",
    "jest": "^27.4.7",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^12.3.3",
    "plop": "^3.0.5",
    "prettier": "^2.4.1",
    "storybook-addon-next-router": "^4.0.0",
    "typescript": "^4.5.5",
    "webpack": "5.68.0"
  }
}
